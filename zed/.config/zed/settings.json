{
  "git_panel": {
    "dock": "right"
  },
  "project_panel": {
    "dock": "left"
  },
  "agent": {
    "always_allow_tool_actions": true,
    "default_model": {
      "provider": "copilot_chat",
      "model": "gpt-4.1"
    },
    "model_parameters": []
  },
  "icon_theme": {
    "mode": "dark",
    "light": "Zed (Default)",
    "dark": "Zed (Default)",
  },
  "vim_mode": true,
  "ui_font_size": 16,
  "buffer_font_size": 16,
  "buffer_font_family": "JetBrains Mono",
  "buffer_font_features": {
    "calt": false
  },
  "ui_font_family": ".SystemUIFont",
  "theme": {
    "dark": "Electron Highlighter",
    "light": "Catppuccin Latte - No Italics"
  },
  "cursor_blink": false,
  "gutter": {
    "folds": false
  }
}
